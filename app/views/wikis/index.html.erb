<div class="page-header">
   <h1>Wikis</h1>
</div>
<div class="row">
   <div class="col-md-8">
      <% @wikis.each do |wiki| %>
         <div class="media">
            <h4 class="media-heading">
               <%= link_to wiki.title, wiki %>
               <% if wiki.private %>
                  <small>Private</small>
               <% end %>
            </h4>
         </div>
      <% end %>
   </div>

   <div class="col-md-4">
      <%= link_to "New", new_wiki_path, class: 'btn btn-success' %>
   </div>

</div>
